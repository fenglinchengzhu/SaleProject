            </RelativeLayout>

            <View style="@style/calculator_input_all_diliver" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="160px"
                android:background="@color/white">

                <TextView
                    android:id="@+id/input_simple_intH_txt"
