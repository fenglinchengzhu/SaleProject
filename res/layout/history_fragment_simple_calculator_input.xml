                    android:layout_toLeftOf="@+id/input_simple_inC_unit"
                    android:layout_toRightOf="@+id/input_simple_inC_txt" />
            </RelativeLayout>
            <!--风区类别-->
            <View style="@style/calculator_input_all_diliver" />

